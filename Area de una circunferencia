using namespace std; 

void main()
{
	float A,pi,r;
	pi=3.1416;
	cout<<"Ingrese el valor del radio";
	cin>> r;
	A=pi*r*r;
  cout<<"El area de la circunferencia es"<<A;
	getch();
}
